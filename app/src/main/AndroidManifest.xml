<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jmw0705.fullscreennotification">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name=".helps.CustomIntentService"
            android:exported="false"
            android:enabled="true"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AlertMessage"></activity>
        <receiver android:name="com.example.jmw0705.fullscreennotification.helps.CustomBroadCastReceiver" >
            <intent-filter>
                <action android:name="com.example.jmw0705.fullscreennotification.USER_ACTION" />
            </intent-filter>
        </receiver>
    </application>

</manifest>